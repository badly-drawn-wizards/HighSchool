<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="rootPane" prefHeight="145.0" prefWidth="311.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fancyshader.client.gui.DetailEditorController">
   <children>
      <TextField onKeyReleased="#onTitleFieldChanged"  fx:id="titleField" layoutX="75.0" layoutY="26.0" prefHeight="25.0" prefWidth="208.0" AnchorPane.leftAnchor="75.0" AnchorPane.rightAnchor="10.0" />
      <TextField fx:id="tagField" layoutX="75.0" layoutY="55.0" prefHeight="25.0" prefWidth="208.0" AnchorPane.leftAnchor="75.0" AnchorPane.rightAnchor="10.0" />
      <ChoiceBox fx:id="availabilityBox" layoutX="75.0" layoutY="84.0" prefHeight="25.0" prefWidth="208.0" AnchorPane.leftAnchor="75.0" AnchorPane.rightAnchor="10.0" />
      <Label layoutX="10.0" layoutY="30.0" text="Title" AnchorPane.leftAnchor="10.0" />
      <Label layoutX="10.0" layoutY="59.0" prefHeight="17.0" prefWidth="34.0" text="Tags" AnchorPane.leftAnchor="10.0" />
      <Label layoutX="10.0" layoutY="88.0" text="Availability" AnchorPane.leftAnchor="10.0" />
      <Button fx:id="doneButton" defaultButton="true" layoutX="104.0" layoutY="113.0" onAction="#onDonePressed" text="Done" />
      <Button cancelButton="true" layoutX="155.0" layoutY="113.0" onAction="#onCancelPressed" text="Cancel" />
      <Label fx:id="topLabel" layoutX="13.0" layoutY="6.0" text="Enter the demo details:" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" />
   </children>
</AnchorPane>
